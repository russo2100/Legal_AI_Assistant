# OpenRouter API
OPENROUTER_API_KEY=your_openrouter_api_key_here

# PostgreSQL с pgvector
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/yurik_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=yurik_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Настройки LLM
OPENROUTER_MODEL=qwen/qwen-2.5-72b-instruct
EMBEDDING_MODEL=qwen/qwen-embed

# Rate limiting (запросы в секунду)
KAD_RATE_LIMIT=0.5
OPENROUTER_RATE_LIMIT=0.5

# Логирование
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# Дисклеймер
DISCLAIMER=Аналитическая подсказка. Требуется проверка юристом.
