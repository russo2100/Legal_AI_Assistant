# OpenRouter API
# Получите ключ на https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-v1-YOUR_API_KEY_HERE

# PostgreSQL с pgvector
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/yurik_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=yurik_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Настройки LLM
OPENROUTER_MODEL=arcee-ai/trinity-large-preview:free
EMBEDDING_MODEL=local

# Rate limiting (запросы в секунду)
KAD_RATE_LIMIT=0.5
OPENROUTER_RATE_LIMIT=0.5

# Логирование
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# Дисклеймер
DISCLAIMER=Аналитическая подсказка. Требуется проверка юристом.
